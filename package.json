    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "vanilla-lazyload": "^12.0.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1"
  },
  "repository": {
