{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "项目描述",
  "private": true,
  "scripts": {
    "// 启动开发服务": "",
    "// start:spa": "当前项目为单页应用开发模式，使用npm run start:spa来启动web dev server",
    "start:spa": "npm start -- --env.spa",
    "// 代码书写规范检查": "",
    "// & and &&区别": "在npm scripts中组合多个命令时，如果使用 && 表示按顺序执行，如果使用 & 表示同时执行。See https://stackoverflow.com/questions/39172536/running-npm-scripts-sequentially",
    "// lint": "对源码目录执行一次代码检查",
    "lint": "npm run lint:js & npm run lint:style & npm run lint:ts",
    "// lint:fix": "尝试使用lint的fix选项修正不符合书写规范的代码，对于无法修正的代码需要自行处理",
    "lint:fix": "npm run lint:js:fix & npm run lint:style:fix & npm run lint:ts:fix",
    "// 编译工程": "",
    "//  测试版本": "",
    "build:staging:clean": "npm run build:production:clean -- --env.SVC_ENV=staging",
    "build:staging:clean:zip": "npm run build:staging:clean -- --env.zip",
    "//  预发环境": "",
    "build:uat:clean": "npm run build:production:clean -- --env.SVC_ENV=uat",
    "build:uat:clean:zip": "npm run build:uat:clean -- --env.zip",
    "//  生产版本": "",
    "build:production:clean": "npm run build:production -- --env.clean",
    "build:production:clean:zip": "npm run build:production:clean -- --env.zip",
    "// 其他scripts": "以下属于不常用的npm scripts，但在遇到特定场景下的问题时将会变得非常又用",
    "start": "webpack-dev-server --mode development --progress --hot --env.clean --env.lint",
    "start:spa:devtool": "npm run start:spa -- --devtool source-map",
    "start:open": "npm start -- --open",
    "build": "npm run build:production",
    "build:development": "webpack --mode development --progress",
    "build:production": "webpack --mode production --progress",
    "build:production:sourcemap": "npm run build:production -- --devtool source-map",
    "build:production:inline-sourcemap": "npm run build:production -- --devtool inline-source-map",
    "build:production:inspect": "npm run build:production -- --debug --env.inspect",
    "build:production:analyze": "npm run build:production -- --env.analyze",
    "build:inspect": "npm run build:development -- -d --env.lint --env.clean",
    "build:inspect:browser": "npm run build:inspect -- --devtool source-map",
    "build:inspect:code": "npm run build:inspect -- --devtool none",
    "build:modernizr": "modernizr --config modernizr-config.json --dest src/library/utils/modernizr.custom.js",
    "build:modernizr:min": "modernizr --config modernizr-config.json --dest src/library/utils/modernizr.custom.min.js --uglify",
    "serve": "ws --compress --directory dist",
    "serve:open": "npm run serve -- --open",
    "serve:spa": "npm run serve -- --spa index.html",
    "lint:js": "eslint ./src --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:style": "stylelint \"./src/**/*.*(p|s|l)+(c|a|e)ss\"",
    "lint:style:fix": "npm run lint:style -- --fix",
    "lint:ts": "tslint --project tsconfig.json",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "// push:all": "一条命令同步多个源代码。 See https://stackoverflow.com/questions/5785549/able-to-push-to-all-git-remotes-with-the-one-command",
    "push:all": "git remote | xargs -L1 git push --all",
    "sonar-scanner": "sonar-scanner -Dproject.settings=./.sonarcloud.properties",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "shinvey@qq.com",
  "license": "ISC",
  "browserslist": [
    "Android >= 4.4",
    "iOS >= 8"
  ],
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "babel-eslint": "^10.0.3",
    "doiuse": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-cache": "^1.1.3",
    "gulp-webp": "^4.0.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "local-web-server": "^3.0.7",
    "modernizr": "^3.8.0",
    "mrm": "^2.0.2",
    "postcss-html": "^0.36.0",
    "postcss-syntax": "^0.36.2",
    "stylelint": "^12.0.0",
    "stylelint-config-lost": "^0.0.3",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-scss": "^3.13.0",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.3.2",
    "cssnano": "^4.1.10",
    "extract-css-chunks-webpack-plugin": "^4.7.1",
    "file-loader": "^5.0.2",
    "filemanager-webpack-plugin": "^2.0.5",
    "glob": "^7.1.6",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-inline-source-plugin": "^1.0.0-beta.2",
    "html-webpack-plugin": "4.0.0-beta.8",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webpack": "^5.1.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lost": "^8.3.1",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "qs": "^6.9.1",
    "responsive-loader": "^1.2.0",
    "rxjs": "^6.5.3",
    "sass-loader": "^8.0.0",
    "sharp": "^0.23.4",
    "style-loader": "^1.0.1",
    "typescript": "^3.7.3",
    "url-loader": "^3.0.0",
    "vanilla-lazyload": "^12.4.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/shinvey/webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/shinvey/webpack-starter/issues"
  },
  "homepage": "https://github.com/shinvey/webpack-starter#readme"
}
