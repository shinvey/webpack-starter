{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "项目描述",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development --hot --devtool source-map --env.clean --env.lint",
    "start:open": "npm start -- --open",
    "start:spa": "npm start -- -d --env.spa",
    "build:development": "webpack --mode development",
    "build:staging": "npm run build:production -- --env.SVC_ENV=staging",
    "build:production": "webpack --mode production",
    "build:production:clean": "npm run build:production -- --env.clean",
    "build:production:sourcemap": "npm run build:production -- --devtool source-map",
    "build:production:inline-sourcemap": "npm run build:production -- --devtool inline-source-map",
    "build:production:inspect": "npm run build:production -- --env.inspect",
    "build:inspect": "npm run build:development -- -d --env.lint --env.clean",
    "build:inspect:browser": "npm run build:inspect -- --devtool source-map",
    "build:inspect:code": "npm run build:inspect -- --devtool none",
    "build:modernizr": "modernizr --config modernizr-config.json --dest src/library/utils/modernizr.custom.js",
    "build:modernizr:min": "modernizr --config modernizr-config.json --dest src/library/utils/modernizr.custom.min.js --uglify",
    "serve": "ws --compress --directory dist",
    "serve:open": "npm run serve -- --open",
    "serve:spa": "npm run serve -- --spa",
    "lint": "eslint . && stylelint src && tslint --project tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "shinvey@qq.com",
  "license": "ISC",
  "browserslist": [
    "Android >= 4.4",
    "iOS >= 8"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "doiuse": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-cache": "^1.1.2",
    "gulp-webp": "^4.0.1",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "local-web-server": "^3.0.7",
    "modernizr": "^3.7.1",
    "mrm": "^1.2.2",
    "rxjs": "^6.5.2",
    "stylelint": "^10.1.0",
    "stylelint-config-lost": "^0.0.3",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylelint-scss": "^3.9.3",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.1.0",
    "cssnano": "^4.1.10",
    "extract-css-chunks-webpack-plugin": "^4.6.0",
    "file-loader": "^4.1.0",
    "filemanager-webpack-plugin": "^2.0.5",
    "glob": "^7.1.4",
    "html-webpack-inline-source-plugin": "^1.0.0-beta.2",
    "html-webpack-plugin": "4.0.0-beta.8",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webpack": "^5.1.0",
    "lost": "^8.3.1",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "responsive-loader": "^1.2.0",
    "sass-loader": "^7.1.0",
    "sharp": "^0.23.0",
