{
  "name": "webpack-spa-react",
  "version": "1.0.0",
  "description": "项目描述",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development --hot --devtool source-map --env.clean --env.lint",
    "start:open": "webpack-dev-server --mode development --hot --devtool source-map --env.clean --env.lint --open",
    "start:spa": "webpack-dev-server --mode development --hot --devtool source-map --env.clean --env.lint --env.spa plugin-based-router.html",
    "build:development": "webpack --mode development",
    "build:staging": "webpack --mode production --env.SVC_ENV=staging",
    "build:production": "webpack --mode production",
    "build:production:clean": "webpack --mode production --env.clean",
    "build:production:sourcemap": "webpack --mode production --devtool source-map",
    "build:production:inline-sourcemap": "webpack --mode production --devtool inline-source-map",
    "build:inspect:browser": "webpack --mode development -d --devtool source-map --env.lint --env.clean",
    "build:inspect:code": "webpack --mode development -d --devtool none --env.lint --env.clean",
    "build:modernizr": "modernizr --config modernizr-config.json --dest src/utils/modernizr.custom.js",
    "build:modernizr:min": "modernizr --config modernizr-config.json --dest src/utils/modernizr.custom.min.js --uglify",
    "serve": "ws --compress --directory dist",
    "serve:open": "ws --compress --open --directory dist",
    "lint": "eslint src --ext .js,.jsx && stylelint src && tslint --project tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "shinvey@qq.com",
  "license": "ISC",
  "browserslist": [
    "Android >= 4.4",
    "iOS >= 8"
  ],
