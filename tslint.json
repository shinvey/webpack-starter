{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  // eslint rules https://eslint.org/docs/rules/
  // tslint rules https://palantir.github.io/tslint/rules/
  "rules": {
    "no-constant-condition": true,
    "no-console": false,
    "semicolon": [true, "never"],
    "space-before-function-paren": false,
    "quotemark": false, // [true, "single", "avoid-escape", "avoid-template", "jsx-double"],
    // 不检查对象字面量属性的排序 https://palantir.github.io/tslint/rules/object-literal-sort-keys/
    "object-literal-sort-keys": false,
    "whitespace": [true, "check-module"],
    "trailing-comma": false,
    "member-access": false,
    "variable-name": false,
    "no-unused-expression": false,
    "arrow-parens": false,
    "ordered-imports": false,
    "comment-format": false,
    "max-line-length": false,
    "no-empty": false,
    "interface-name": false
  },
  "rulesDirectory": [
    // Improve your TSLint with the missing ESLint rules
    // https://github.com/buzinas/tslint-eslint-rules
    // 支持的rules列表 https://github.com/buzinas/tslint-eslint-rules#rules-copied-from-the-eslint-website
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
